{
  "name": "@baeta/plugin",
  "version": "0.0.12",
  "license": "MIT",
  "author": "Andrei Pampu <pampu.andrei@pm.me>",
  "homepage": "https://github.com/andreisergiu98/baeta#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/andreisergiu98/baeta.git",
    "directory": "packages/plugin"
  },
  "bugs": {
    "url": "https://github.com/andreisergiu98/baeta/issues"
  },
  "type": "module",
  "scripts": {
    "build": "tsup",
    "types": "tsc --noEmit",
    "prepack": "yarn prep --copy dist",
    "postpack": "yarn prep --clean"
  },
  "dependencies": {
    "@baeta/config": "workspace:^"
  },
  "devDependencies": {
    "@baeta/build-tools": "workspace:^",
    "@types/node": "^18.11.18",
    "typescript": "^4.9.4"
  },
  "files": [
    "*.js",
    "*.cjs",
    "*.d.ts",
    "!ava.config.js"
  ],
  "publishConfig": {
    "access": "public",
    "publishDir": "dist",
    "exports": {
      ".": {
        "import": "./index.js",
        "require": "./index.cjs",
        "types": "./index.d.ts"
      }
    }
  }
}
