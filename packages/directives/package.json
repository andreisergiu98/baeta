{
  "name": "@baeta/directives",
  "version": "1.0.4",
  "keywords": [
    "baeta",
    "graphql",
    "schema",
    "types",
    "typescript",
    "framework",
    "builder"
  ],
  "homepage": "https://github.com/andreisergiu98/baeta#readme",
  "bugs": {
    "url": "https://github.com/andreisergiu98/baeta/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andreisergiu98/baeta.git",
    "directory": "packages/directives"
  },
  "license": "MIT",
  "author": {
    "name": "Andrei Pampu",
    "url": "https://github.com/andreisergiu98"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.ts",
      "default": "./dist/index.js"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "build": "tsup",
    "prepack": "prep",
    "postpack": "prep --clean",
    "test": "ava",
    "types": "tsc --noEmit"
  },
  "dependencies": {
    "@baeta/errors": "workspace:^",
    "email-validator": "^2.0.4",
    "is-url": "^1.2.4"
  },
  "devDependencies": {
    "@baeta/builder": "workspace:^",
    "@baeta/core": "workspace:^",
    "@baeta/tsconfig": "workspace:^",
    "@graphql-tools/schema": "^10.0.11",
    "@types/is-url": "^1.2.32",
    "ava": "^6.2.0",
    "graphql": "^16.9.0",
    "graphql-tag": "^2.12.6",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "@baeta/core": "workspace:^",
    "graphql": "^16.6.0"
  },
  "engines": {
    "node": ">=22.12.0"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--no-warnings",
      "--experimental-transform-types"
    ]
  },
  "typedocOptions": {
    "entryPoints": [
      "./index.ts"
    ],
    "readme": "none",
    "tsconfig": "./tsconfig.json"
  }
}
