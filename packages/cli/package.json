{
  "name": "@baeta/cli",
  "version": "0.0.21",
  "license": "MIT",
  "author": "Andrei Pampu <pampu.andrei@pm.me>",
  "homepage": "https://github.com/andreisergiu98/baeta#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/andreisergiu98/baeta.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/andreisergiu98/baeta/issues"
  },
  "type": "module",
  "scripts": {
    "build": "tsup",
    "types": "tsc --noEmit",
    "prepack": "yarn prep --copy dist",
    "postpack": "yarn prep --clean"
  },
  "dependencies": {
    "@baeta/config": "workspace:^",
    "@baeta/generator": "workspace:^",
    "@baeta/plugin": "workspace:^",
    "@graphql-codegen/core": "^2.6.8",
    "@graphql-codegen/plugin-helpers": "^3.1.2",
    "@graphql-codegen/typescript": "^2.8.7",
    "@graphql-codegen/visitor-plugin-common": "2.13.7",
    "@graphql-tools/apollo-engine-loader": "^7.3.22",
    "@graphql-tools/code-file-loader": "^7.3.16",
    "@graphql-tools/git-loader": "^7.2.16",
    "@graphql-tools/github-loader": "^7.3.23",
    "@graphql-tools/graphql-file-loader": "^7.5.14",
    "@graphql-tools/json-file-loader": "^7.4.15",
    "@graphql-tools/load": "^7.8.9",
    "@graphql-tools/prisma-loader": "^7.2.52",
    "@graphql-tools/url-loader": "^7.17.1",
    "@graphql-tools/utils": "^9.1.4",
    "auto-bind": "^5.0.1",
    "chalk": "^5.2.0",
    "change-case-all": "1.0.15",
    "chokidar": "^3.5.3",
    "cli-spinners": "^2.7.0",
    "glob": "^8.1.0",
    "ink": "^3.2.0",
    "nanoid": "^4.0.0",
    "parse-filepath": "^1.0.2",
    "react": "^17.0.2",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "yargs": "^17.6.2"
  },
  "peerDependencies": {
    "graphql": "^16.6.0"
  },
  "optionalDependencies": {
    "@baeta/compiler": "workspace:^"
  },
  "devDependencies": {
    "@baeta/build-tools": "workspace:^",
    "@baeta/core": "workspace:^",
    "@types/glob": "^8",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/yargs": "^17.0.19",
    "graphql": "^16.6.0",
    "typescript": "^4.9.4"
  },
  "bin": {
    "baeta": "./bin/cli-dev.cjs"
  },
  "files": [
    "*.js",
    "*.cjs",
    "*.d.ts",
    "!ava.config.js"
  ],
  "publishConfig": {
    "access": "public",
    "publishDir": "dist",
    "bin": {
      "baeta": "./bin/cli.cjs"
    },
    "exports": {
      ".": {
        "import": "./index.js",
        "require": "./index.cjs",
        "types": "./index.d.ts"
      }
    }
  }
}
