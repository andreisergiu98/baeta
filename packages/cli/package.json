{
  "name": "@baeta/cli",
  "version": "2.0.0-next.0",
  "keywords": [
    "baeta",
    "graphql",
    "schema",
    "types",
    "typescript",
    "framework",
    "builder"
  ],
  "homepage": "https://github.com/andreisergiu98/baeta#readme",
  "bugs": {
    "url": "https://github.com/andreisergiu98/baeta/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andreisergiu98/baeta.git",
    "directory": "packages/cli"
  },
  "license": "MIT",
  "author": {
    "name": "Andrei Pampu",
    "url": "https://github.com/andreisergiu98"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.ts",
      "default": "./index.ts"
    },
    "./sdk": {
      "types": "./sdk/index.ts",
      "default": "./sdk/index.ts"
    }
  },
  "types": "dist/index.d.ts",
  "bin": {
    "baeta": "./bin/cli-dev.js"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "build": "tsup",
    "prepack": "prep",
    "postpack": "prep --clean",
    "types": "tsc --noEmit"
  },
  "dependencies": {
    "@baeta/generator": "workspace:^",
    "@baeta/plugin-graphql": "workspace:^",
    "@baeta/util-path": "workspace:^",
    "@xterm/addon-serialize": "^0.13.0",
    "@xterm/headless": "^5.5.0",
    "@xterm/xterm": "^5.5.0",
    "ansi-styles": "^6.2.3",
    "fast-glob": "^3.3.3",
    "ink": "^6.3.1",
    "jiti": "^2.6.1",
    "lodash.throttle": "^4.1.1",
    "node-pty": "^1.0.0",
    "react": "^19.2.0",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@baeta/builder": "workspace:^",
    "@baeta/tsconfig": "workspace:^",
    "@types/lodash.throttle": "^4.1.9",
    "@types/node": "^22.18.11",
    "@types/react": "^19.2.2",
    "@types/yargs": "^17.0.33",
    "graphql": "^16.11.0",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "graphql": "^16.6.0"
  },
  "engines": {
    "node": ">=22.20.0"
  },
  "publishConfig": {
    "access": "public",
    "bin": {
      "baeta": "./dist/bin/cli.js"
    },
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "./sdk": {
        "types": "./dist/sdk/index.d.ts",
        "default": "./dist/sdk/index.js"
      }
    }
  },
  "typedocOptions": {
    "entryPoints": [
      "./index.ts",
      "./sdk/index.ts"
    ],
    "readme": "none",
    "tsconfig": "./tsconfig.json",
    "sort": [
      "kind",
      "instance-first",
      "required-first",
      "alphabetical-ignoring-documents"
    ]
  }
}
