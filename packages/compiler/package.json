{
  "name": "@baeta/compiler",
  "version": "0.1.4",
  "keywords": [
    "baeta",
    "graphql",
    "schema",
    "types",
    "typescript",
    "framework",
    "builder"
  ],
  "homepage": "https://github.com/andreisergiu98/baeta#readme",
  "bugs": {
    "url": "https://github.com/andreisergiu98/baeta/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andreisergiu98/baeta.git",
    "directory": "packages/compiler"
  },
  "license": "MIT",
  "author": {
    "name": "Andrei Pampu",
    "url": "https://github.com/andreisergiu98"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.ts",
      "default": "./dist/index.js"
    },
    "./esbuild": {
      "types": "./esbuild/index.ts",
      "default": "./dist/esbuild/index.js"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "build": "tsup",
    "prepack": "prep",
    "postpack": "prep --clean",
    "types": "tsc --noEmit"
  },
  "dependencies": {
    "@baeta/util-path": "workspace:^",
    "esbuild": "^0.24.0"
  },
  "devDependencies": {
    "@baeta/builder": "workspace:^",
    "@baeta/tsconfig": "workspace:^",
    "@types/node": "^22.10.1",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=22.12.0"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "./esbuild": {
        "types": "./dist/esbuild/index.d.ts",
        "default": "./dist/esbuild/index.js"
      }
    }
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--no-warnings",
      "--experimental-transform-types"
    ]
  },
  "typedocOptions": {
    "entryPoints": [
      "./index.ts",
      "./esbuild/index.ts"
    ],
    "readme": "none",
    "tsconfig": "./tsconfig.json"
  }
}
