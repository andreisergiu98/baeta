{
  "name": "@baeta/cli",
  "version": "0.0.12",
  "license": "MIT",
  "author": "Andrei Pampu <pampu.andrei@pm.me>",
  "homepage": "https://github.com/andreisergiu98/baeta#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/andreisergiu98/baeta.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/andreisergiu98/baeta/issues"
  },
  "type": "module",
  "scripts": {
    "build": "tsup",
    "types": "tsc --noEmit",
    "prepack": "cp package.json dist/package.json"
  },
  "dependencies": {
    "@baeta/config": "workspace:^",
    "@baeta/generator": "workspace:^",
    "@baeta/plugin": "workspace:^",
    "chalk": "^5.1.2",
    "chokidar": "^3.5.3",
    "cli-spinners": "^2.7.0",
    "ink": "^3.2.0",
    "nanoid": "^4.0.0",
    "react": "^17.0.2",
    "yargs": "^17.6.2"
  },
  "peerDependencies": {
    "graphql": "^15.3.0 || ^16.0.0"
  },
  "optionalDependencies": {
    "@baeta/compiler": "workspace:^"
  },
  "devDependencies": {
    "@baeta/core": "workspace:^",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/yargs": "^17.0.15",
    "graphql": "^16.6.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.3"
  },
  "bin": {
    "baeta": "./bin/cli-dev.cjs"
  },
  "files": [
    "*.js",
    "*.cjs",
    "*.d.ts"
  ],
  "publishConfig": {
    "access": "public",
    "directory": "dist",
    "bin": {
      "baeta": "./cli.js"
    },
    "exports": {
      ".": {
        "import": "./index.js",
        "require": "./index.cjs",
        "types": "./index.d.ts"
      }
    }
  }
}
