{
  "name": "@baeta/plugin",
  "version": "0.0.4",
  "type": "module",
  "scripts": {
    "prepack": "yarn prep --move dist",
    "postpack": "yarn prep --clean",
    "build": "yarn tsup --dts"
  },
  "devDependencies": {
    "@baeta/core": "workspace:^",
    "@baeta/generator": "workspace:^",
    "@baeta/prep": "workspace:^",
    "@types/node": "^18.7.23",
    "typescript": "^4.8.3"
  },
  "files": [
    "*.js",
    "*.cjs",
    "*.d.ts"
  ],
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "import": "./index.js",
        "require": "./index.cjs",
        "types": "./index.d.ts"
      }
    }
  }
}
