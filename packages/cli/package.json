    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@baeta/core": "workspace:^",
    "@baeta/prep": "workspace:^",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/yargs": "^17.0.13",
    "graphql": "^16.6.0",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "@baeta/core": "workspace:^",
    "graphql": "^15.3.0 || ^16.0.0"
  },
  "optionalDependencies": {
    "@baeta/compiler": "workspace:^"
  },
  "bin": {
