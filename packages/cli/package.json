{
  "name": "@baeta/cli",
  "version": "0.0.22",
  "license": "MIT",
  "author": "Andrei Pampu <pampu.andrei@pm.me>",
  "homepage": "https://github.com/andreisergiu98/baeta#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/andreisergiu98/baeta.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/andreisergiu98/baeta/issues"
  },
  "type": "module",
  "scripts": {
    "build": "tsup",
    "types": "tsc --noEmit",
    "prepack": "yarn prep --copy dist",
    "postpack": "yarn prep --clean"
  },
  "dependencies": {
    "@baeta/config": "workspace:^",
    "@baeta/generator": "workspace:^",
    "@baeta/plugin": "workspace:^",
    "ansi-styles": "^6.2.1",
    "chokidar": "^3.5.3",
    "cli-spinners": "^2.7.0",
    "execa": "^6.1.0",
    "fast-glob": "^3.2.12",
    "ink": "^3.2.0",
    "nanoid": "^4.0.0",
    "react": "^17.0.2",
    "tree-kill": "^1.2.2",
    "yargs": "^17.6.2"
  },
  "peerDependencies": {
    "graphql": "^16.6.0"
  },
  "optionalDependencies": {
    "@baeta/compiler": "workspace:^"
  },
  "devDependencies": {
    "@baeta/build-tools": "workspace:^",
    "@baeta/core": "workspace:^",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/yargs": "^17.0.19",
    "graphql": "^16.6.0",
    "typescript": "^4.9.4"
  },
  "bin": {
    "baeta": "./bin/cli-dev.cjs"
  },
  "files": [
    "*.js",
    "*.cjs",
    "*.d.ts",
    "!ava.config.js"
  ],
  "publishConfig": {
    "access": "public",
    "publishDir": "dist",
    "bin": {
      "baeta": "./bin/cli.js"
    },
    "exports": {
      ".": {
        "import": "./index.js",
        "require": "./index.cjs",
        "types": "./index.d.ts"
      }
    }
  }
}
