{
  "name": "@baeta/core",
  "version": "0.1.2",
  "keywords": [
    "baeta",
    "graphql",
    "schema",
    "types",
    "typescript",
    "framework",
    "builder"
  ],
  "homepage": "https://github.com/andreisergiu98/baeta#readme",
  "bugs": {
    "url": "https://github.com/andreisergiu98/baeta/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andreisergiu98/baeta.git",
    "directory": "packages/core"
  },
  "license": "MIT",
  "author": {
    "name": "Andrei Pampu",
    "url": "https://github.com/andreisergiu98"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./sdk": {
      "types": "./sdk/index.ts",
      "import": "./dist/sdk/index.js",
      "require": "./dist/sdk/index.cjs"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "build": "tsup",
    "prepack": "prep",
    "postpack": "prep --clean",
    "test": "ava",
    "types": "tsc --noEmit"
  },
  "dependencies": {
    "@baeta/errors": "workspace:^",
    "@graphql-tools/schema": "^10.0.8",
    "@graphql-tools/utils": "^10.5.6"
  },
  "devDependencies": {
    "@baeta/builder": "workspace:^",
    "@baeta/tsconfig": "workspace:^",
    "ava": "^6.2.0",
    "graphql": "^16.9.0",
    "graphql-tag": "^2.12.6",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "graphql": "^16.6.0"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      },
      "./sdk": {
        "types": "./dist/sdk/index.d.ts",
        "import": "./dist/sdk/index.js",
        "require": "./dist/sdk/index.cjs"
      }
    }
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--no-warnings",
      "--experimental-transform-types"
    ]
  },
  "typedocOptions": {
    "entryPoints": [
      "./index.ts",
      "./sdk/index.ts"
    ],
    "readme": "none",
    "tsconfig": "./tsconfig.json"
  }
}
