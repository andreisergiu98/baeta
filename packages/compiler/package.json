{
  "name": "@baeta/compiler",
  "version": "1.0.9",
  "keywords": [
    "baeta",
    "graphql",
    "schema",
    "types",
    "typescript",
    "framework",
    "builder"
  ],
  "homepage": "https://github.com/andreisergiu98/baeta#readme",
  "bugs": {
    "url": "https://github.com/andreisergiu98/baeta/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andreisergiu98/baeta.git",
    "directory": "packages/compiler"
  },
  "license": "MIT",
  "author": {
    "name": "Andrei Pampu",
    "url": "https://github.com/andreisergiu98"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.ts",
      "default": "./index.ts"
    },
    "./esbuild": {
      "types": "./esbuild/index.ts",
      "default": "./esbuild/index.ts"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "build": "tsup",
    "prepack": "prep",
    "postpack": "prep --clean",
    "types": "tsc --noEmit"
  },
  "dependencies": {
    "@baeta/util-path": "workspace:^",
    "esbuild": "^0.25.10"
  },
  "devDependencies": {
    "@baeta/builder": "workspace:^",
    "@baeta/tsconfig": "workspace:^",
    "@types/node": "^22.18.11",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=22.12.0"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "./esbuild": {
        "types": "./dist/esbuild/index.d.ts",
        "default": "./dist/esbuild/index.js"
      }
    }
  },
  "typedocOptions": {
    "entryPoints": [
      "./index.ts",
      "./esbuild/index.ts"
    ],
    "readme": "none",
    "tsconfig": "./tsconfig.json",
    "sort": [
      "kind",
      "instance-first",
      "required-first",
      "alphabetical-ignoring-documents"
    ]
  }
}
