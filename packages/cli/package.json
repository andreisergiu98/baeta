{
  "name": "@baeta/cli",
  "version": "0.0.39",
  "keywords": [
    "baeta",
    "graphql",
    "schema",
    "types",
    "typescript",
    "framework",
    "builder"
  ],
  "homepage": "https://github.com/andreisergiu98/baeta#readme",
  "bugs": {
    "url": "https://github.com/andreisergiu98/baeta/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andreisergiu98/baeta.git",
    "directory": "packages/cli"
  },
  "license": "MIT",
  "author": {
    "name": "Andrei Pampu",
    "url": "https://github.com/andreisergiu98"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./ink": {
      "import": "./dist/ink/index.js",
      "require": "./dist/ink/index.cjs",
      "types": "./dist/ink/index.d.ts"
    },
    "./sdk": {
      "import": "./dist/sdk/index.js",
      "require": "./dist/sdk/index.cjs",
      "types": "./dist/sdk/index.d.ts"
    }
  },
  "types": "dist/index.d.ts",
  "bin": {
    "baeta": "./dist/bin/cli.js"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepack": "prep",
    "postpack": "prep --clean",
    "types": "tsc --noEmit"
  },
  "dependencies": {
    "@baeta/generator": "workspace:^",
    "@baeta/plugin-graphql": "workspace:^",
    "ansi-styles": "^6.2.1",
    "cli-spinners": "^2.9.2",
    "execa": "^7.2.0",
    "fast-glob": "^3.3.2",
    "ink": "^3.2.0",
    "nanoid": "^4.0.2",
    "react": "^17.0.2",
    "tree-kill": "^1.2.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@baeta/builder": "workspace:^",
    "@baeta/compiler": "workspace:^",
    "@baeta/tsconfig": "workspace:^",
    "@types/node": "^18.19.0",
    "@types/react": "^17.0.71",
    "@types/yargs": "^17.0.32",
    "graphql": "^16.8.1",
    "typescript": "^5.3.2"
  },
  "peerDependencies": {
    "@baeta/compiler": "workspace:^",
    "graphql": "^16.6.0"
  },
  "peerDependenciesMeta": {
    "@baeta/compiler": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
