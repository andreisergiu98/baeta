{
  "name": "@baeta/plugin-graphql",
  "version": "0.0.7",
  "license": "MIT",
  "author": "Andrei Pampu <pampu.andrei@pm.me>",
  "homepage": "https://github.com/andreisergiu98/baeta#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/andreisergiu98/baeta.git",
    "directory": "plugins/graphql"
  },
  "bugs": {
    "url": "https://github.com/andreisergiu98/baeta/issues"
  },
  "type": "module",
  "scripts": {
    "build": "tsup --dts",
    "types": "tsc --noEmit",
    "prepack": "prep --move dist",
    "postpack": "prep --clean"
  },
  "dependencies": {
    "@baeta/plugin": "workspace:^",
    "@graphql-codegen/core": "^2.6.5",
    "@graphql-codegen/plugin-helpers": "^2.7.2",
    "@graphql-codegen/typescript": "^2.8.1",
    "@graphql-codegen/typescript-resolvers": "^2.7.6",
    "@graphql-codegen/visitor-plugin-common": "2.13.1",
    "@graphql-tools/apollo-engine-loader": "^7.3.18",
    "@graphql-tools/code-file-loader": "^7.3.11",
    "@graphql-tools/git-loader": "^7.2.11",
    "@graphql-tools/github-loader": "^7.3.18",
    "@graphql-tools/graphql-file-loader": "^7.5.10",
    "@graphql-tools/json-file-loader": "^7.4.11",
    "@graphql-tools/load": "^7.8.5",
    "@graphql-tools/prisma-loader": "^7.2.34",
    "@graphql-tools/url-loader": "^7.16.14",
    "@graphql-tools/utils": "^9.1.0",
    "auto-bind": "^5.0.1",
    "change-case-all": "1.0.15",
    "parse-filepath": "^1.0.2",
    "tslib": "^2.4.1"
  },
  "devDependencies": {
    "@baeta/prep": "workspace:^",
    "@types/node": "^18.11.9",
    "@types/parse-filepath": "1.0.0",
    "graphql": "^16.6.0",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "@baeta/core": "workspace:^",
    "graphql": "^15.3.0 || ^16.0.0"
  },
  "files": [
    "*.js",
    "*.cjs",
    "*.d.ts"
  ],
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "import": "./index.js",
        "require": "./index.cjs",
        "types": "./index.d.ts"
      }
    }
  }
}
