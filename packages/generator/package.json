{
  "name": "@baeta/generator",
  "version": "0.0.6",
  "license": "MIT",
  "author": "Andrei Pampu <pampu.andrei@pm.me>",
  "homepage": "https://github.com/andreisergiu98/baeta#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/andreisergiu98/baeta.git",
    "directory": "packages/generator"
  },
  "bugs": {
    "url": "https://github.com/andreisergiu98/baeta/issues"
  },
  "type": "module",
  "scripts": {
    "build": "tsup",
    "types": "tsc --noEmit",
    "prepack": "cp package.json dist/package.json"
  },
  "dependencies": {
    "@baeta/config": "workspace:^",
    "@baeta/plugin": "workspace:^",
    "@baeta/plugin-graphql": "workspace:^",
    "chokidar": "^3.5.3"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "graphql": "^16.6.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.3"
  },
  "peerDependencies": {
    "graphql": "^15.3.0 || ^16.0.0"
  },
  "files": [
    "*.js",
    "*.cjs",
    "*.d.ts"
  ],
  "publishConfig": {
    "access": "public",
    "directory": "dist",
    "exports": {
      ".": {
        "import": "./index.js",
        "require": "./index.cjs",
        "types": "./index.d.ts"
      }
    }
  }
}
