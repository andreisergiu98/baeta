{
  "name": "create-baeta",
  "version": "1.0.12",
  "keywords": [
    "baeta",
    "graphql",
    "schema",
    "types",
    "typescript",
    "framework",
    "builder"
  ],
  "homepage": "https://github.com/andreisergiu98/baeta#readme",
  "bugs": {
    "url": "https://github.com/andreisergiu98/baeta/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andreisergiu98/baeta.git",
    "directory": "packages/create-baeta"
  },
  "license": "MIT",
  "author": {
    "name": "Andrei Pampu",
    "url": "https://github.com/andreisergiu98"
  },
  "type": "module",
  "bin": "dist/cli.js",
  "exports": {
    ".": {
      "types": "./index.ts",
      "default": "./dist/index.js"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "templates",
    "package.json"
  ],
  "scripts": {
    "prebuild": "node --no-warnings --experimental-transform-types ./scripts/version-manifest.ts",
    "build": "yarn prebuild && tsup",
    "prepack": "prep",
    "postpack": "prep --clean",
    "types": "yarn prebuild && tsc --noEmit"
  },
  "dependencies": {
    "@docusaurus/logger": "^3.8.0",
    "fs-extra": "^11.3.0",
    "prompts": "^2.4.2",
    "semver": "^7.7.2",
    "shelljs": "^0.10.0",
    "supports-color": "^10.0.0",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@baeta/builder": "workspace:^",
    "@baeta/tsconfig": "workspace:^",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.15.30",
    "@types/prompts": "^2.4.9",
    "@types/semver": "^7.7.0",
    "@types/shelljs": "^0.8.16",
    "@types/yargs": "^17.0.33",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=22.12.0"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "typedocOptions": {
    "entryPoints": [
      "./index.ts"
    ],
    "readme": "none",
    "tsconfig": "./tsconfig.json",
    "sort": [
      "kind",
      "instance-first",
      "required-first",
      "alphabetical-ignoring-documents"
    ]
  }
}
