{
  "name": "@baeta/core",
  "version": "0.0.11",
  "type": "module",
  "scripts": {
    "prepack": "yarn prep --move dist",
    "postpack": "yarn prep --clean",
    "build": "yarn tsup --dts"
  },
  "dependencies": {
    "@graphql-tools/schema": "^9.0.4",
    "@graphql-tools/utils": "^8.12.0",
    "graphql-modules": "^2.1.0",
    "graphql-subscriptions": "^2.0.0"
  },
  "devDependencies": {
    "@baeta/compiler": "workspace:^",
    "@baeta/generator": "workspace:^",
    "@baeta/plugin": "workspace:^",
    "@baeta/plugin-graphql": "workspace:^",
    "@baeta/prep": "workspace:^",
    "@types/node": "^18.7.23",
    "graphql": "^16.6.0",
    "typescript": "^4.8.3"
  },
  "peerDependencies": {
    "graphql": "^15.3.0 || ^16.0.0"
  },
  "files": [
    "*.js",
    "*.cjs",
    "*.d.ts"
  ],
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "import": "./index.js",
        "require": "./index.cjs",
        "types": "./index.d.ts"
      },
      "./sdk": {
        "import": "./sdk/index.js",
        "require": "./sdk/index.cjs",
        "types": "./sdk/index.d.ts"
      }
    }
  }
}
