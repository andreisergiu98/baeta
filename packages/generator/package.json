{
  "name": "@baeta/generator",
  "version": "0.0.4",
  "type": "module",
  "scripts": {
    "prepack": "yarn prep --move dist",
    "postpack": "yarn prep --clean",
    "build": "yarn tsup --dts"
  },
  "dependencies": {
    "@baeta/plugin": "workspace:^",
    "@baeta/plugin-graphql": "workspace:^",
    "chokidar": "^3.5.3"
  },
  "devDependencies": {
    "@baeta/core": "workspace:^",
    "@baeta/prep": "workspace:^",
    "@types/node": "^18.7.23",
    "graphql": "^16.6.0",
    "typescript": "^4.8.3"
  },
  "peerDependencies": {
    "@baeta/core": "workspace:^",
    "graphql": "^15.3.0 || ^16.0.0"
  },
  "files": [
    "*.js",
    "*.cjs",
    "*.d.ts"
  ],
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "import": "./index.js",
        "require": "./index.cjs",
        "types": "./index.d.ts"
      }
    }
  }
}
