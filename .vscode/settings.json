{
  "search.exclude": {
    "**/.docusaurus": true,
    "**/.pnp.*": true,
    "**/.turbo": true,
    "**/.yarn": true,
    "**/dist": true,
    "website/build": true
  },
  "files.exclude": {
    "**/.docusaurus": true,
    "**/.turbo": true,
    "**/node_modules": true,
    "**/dist": true,
    "website/build": true,
    "website/.yarn": true
  },
  "cSpell.words": [
    "autoload",
    "autoloading",
    "baeta",
    "callsite",
    "camelcase",
    "clsx",
    "codegen",
    "datamodel",
    "dirpath",
    "dmmf",
    "execa",
    "keyv",
    "middlewares",
    "outdir",
    "unnormalized",
    "upstash"
  ],
  "typescript.tsdk": ".yarn/sdks/typescript/lib",
  "prettier.prettierPath": ".yarn/sdks/prettier/index.cjs",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescript.preferences.importModuleSpecifierEnding": "js",
  "[typescript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[javascript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[json]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[graphql]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[css]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[mdx]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
