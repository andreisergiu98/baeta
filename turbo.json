{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**/*"
      ]
    },
    "test": {
      "dependsOn": [
        "^build"
      ]
    },
    "types": {
      "dependsOn": [
        "^types"
      ]
    }
  }
}
