{
  "$schema": "https://unpkg.com/@changesets/config@2.2.0/schema.json",
  "changelog": [
    "@changesets/changelog-github",
    {
      "repo": "andreisergiu98/baeta"
    }
  ],
  "commit": false,
  "access": "public",
  "baseBranch": "main",
  "updateInternalDependencies": "patch",
  "snapshot": {
    "useCalculatedVersion": false,
    "prereleaseTemplate": "{tag}-{datetime}-{commit}"
  },
  "fixed": [],
  "linked": [
    [
      "create-baeta",
      "@baeta/cli",
      "@baeta/compiler",
      "@baeta/core",
      "@baeta/directives",
      "@baeta/env",
      "@baeta/errors",
      "@baeta/extension-auth",
      "@baeta/plugin-autoload",
      "@baeta/plugin-directives",
      "@baeta/plugin-exec",
      "@baeta/plugin-gitignore",
      "@baeta/plugin-graphql",
      "@baeta/plugin-pagination",
      "@baeta/plugin-prisma",
      "@baeta/subscriptions-pubsub"
    ]
  ],
  "ignore": [
    "@baeta/examples*",
    "@baeta/template*"
  ]
}
